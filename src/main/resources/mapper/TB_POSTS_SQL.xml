<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="TB_POSTS">
	<insert id="insert" parameterType="com.mvc.spring.dto.PostCreateRequestDTO" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO TB_POSTS (
			 title
			,content
			,original_filename
			,stored_filename
			,date_dir
		)
		VALUES (
			 #{title}
			,#{content}
			,#{originalFilename}
			,#{storedFilename}
			,#{dateDir}
		)
	</insert>
</mapper>